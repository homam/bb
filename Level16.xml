<?xml version="1.0" encoding="utf-8"?>
<level xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../level.xsd">
  <levelInfo number="16">
    <name>
      <en>Airport</en>
      <ar>
        <x>المطار</x>
        <r>almataar</r>
      </ar>
      <de>Flughafen </de>
      <pl>Lotnisko</pl>
      <es>Aeropuerto</es>
    </name>
  </levelInfo>
  <steps>
    <step>
      <word wid="1601" type="word">
        <translations>
          <en>Airport</en>
          <ar>
            <x>المطار</x>
            <r>almataar</r>
          </ar>
          <de>Flughafen</de>
          <pl>Lotnisko</pl>
          <es>Aeropuerto</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1602" type="word">
        <translations>
          <en>Flight number</en>
          <ar>
            <x>الرحلة رقم</x>
            <r>alrihla rakam</r>
          </ar>
          <de>Flugnummer</de>
          <pl>Numer lotu</pl>
          <es>Número de vuelo</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1603" type="word">
        <translations>
          <en>Take me to the airport.</en>
          <ar>
            <x>خذني إلى المطار</x>
            <r>khuthni ila almataar</r>
          </ar>
          <de>Bringen Sie mich zum Flughafen</de>
          <pl>Proszę zawieźć mnie na lotnisko.</pl>
          <es>Lléveme al aeropuerto.</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1604" type="word">
        <translations>
          <en>Luggage</en>
          <ar>
            <x>أمتعة</x>
            <r>amtia'a</r>
          </ar>
          <de>Gepäck</de>
          <pl>Bagaż</pl>
          <es>Equipaje</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1605" type="word">
        <translations>
          <en>Gate</en>
          <ar>
            <x>بوابة</x>
            <r>bawaba</r>
          </ar>
          <de>Flugsteig</de>
          <pl>Bramka</pl>
          <es>Puerta de embarque</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1606" type="word">
        <translations>
          <en>Ticket</en>
          <ar>
            <x>تذكرة</x>
            <r>tathkara</r>
          </ar>
          <de>Ticket</de>
          <pl>Bilet</pl>
          <es>Billete</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1607" type="word">
        <translations>
          <en>Plane</en>
          <ar>
            <x>طائرة</x>
            <r>taira</r>
          </ar>
          <de>Flugzeug</de>
          <pl>Samolot</pl>
          <es>Avión</es>
        </translations>
      </word>
    </step>
    <step>
      <multipleChoiceTest limitTestsBy="2">
        <choicesByRef>
          <choice ref="1604" />
          <choice ref="1606" />
          <choice ref="1605" />
          <choice ref="1607" />
        </choicesByRef>
      </multipleChoiceTest>
    </step>
    <step>
      <word wid="1608" type="word">
        <translations>
          <en>This is my luggage.</en>
          <ar>
            <x>هذه أمتعتي</x>
            <r>hathihi amtia'ati</r>
          </ar>
          <de>Dies ist meine Gepäck.</de>
          <pl>To jest mój bagaż.</pl>
          <es>Este es mi equipaje.</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1609" type="word">
        <translations>
          <en>This is my bag.</en>
          <ar>
            <x>هذه حقيبتي</x>
            <r>hathihi hakibati</r>
          </ar>
          <de>Das ist meine Tasche.</de>
          <pl>To jest moja torba.</pl>
          <es>Este es mi bolso.</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1610" type="word">
        <translations>
          <en>Where are the duty free shops?</en>
          <ar>
            <x>أين السوق الحرة؟</x>
            <r>ayna alsouk alhura?</r>
          </ar>
          <de>Wo sind die Duty Free Shops?</de>
          <pl>Gdzie są sklepy wolnocłowe?</pl>
          <es>¿Dónde están las tiendas libres de impuestos?</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1611" type="word">
        <translations>
          <en>Where is gate number...?</en>
          <ar>
            <x>أين البوابة رقم..</x>
            <r>ayna albawaba rakam...</r>
          </ar>
          <de>Wo ist der Flugsteig Nummer ...?</de>
          <pl>Gdzie jest bramka numer…?</pl>
          <es>¿Dónde está el número de la puerta de embarque…?</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1614" type="word">
        <translations>
          <en>Departure</en>
          <ar>
            <x>مغادرة</x>
            <r>moghadara</r>
          </ar>
          <de>Abflug</de>
          <pl>Odlot</pl>
          <es>Salida</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1615" type="word">
        <translations>
          <en>Arrival</en>
          <ar>
            <x>وصول</x>
            <r>ousoul</r>
          </ar>
          <de>Ankunft</de>
          <pl>Przylot</pl>
          <es>Llegada</es>
        </translations>
      </word>
    </step>
    <step>
      <multipleChoiceTest limitTestsBy="1">
        <choicesByRef>
          <choice ref="1614" />
          <choice ref="1615" />
        </choicesByRef>
      </multipleChoiceTest>
    </step>
    <step>
      <word wid="1612" type="word">
        <translations>
          <en>When is the departure?</en>
          <ar>
            <x>متى وقت المغادرة؟</x>
            <r>mata wakt almughadra?</r>
          </ar>
          <de>Wann ist der Abflug?</de>
          <pl>O której jest odlot?</pl>
          <es>¿Cuándo es la salida?</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1613" type="word">
        <translations>
          <en>What time are we expected to arrive?</en>
          <ar>
            <x>ما الوقت المتوقع للوصول؟</x>
            <r>ma alwakt almutawaka'a lilousoul?</r>
          </ar>
          <de>Zu welcher Zeit werden wir etwa ankommen?</de>
          <pl>O której przylecimy?</pl>
          <es>¿A qué hora está previsto que lleguemos.?</es>
        </translations>
      </word>
    </step>
    <step>
      <dialog did="1616">
        <among>
          <speaker who="A" gender="M"></speaker>
          <speaker who="B" gender="F"></speaker>
        </among>
        <lines>
          <line by="A">
            <translations>
              <en>Where is the duty free shops?</en>
              <ar>
                <x>أين السوق الحرة؟</x>
                <r>ayna alsouk alhurra?</r>
              </ar>
              <de>Wo befinden sich die Duty Free Shops?</de>
              <pl>Gdzie są sklepy wolnocłowe?</pl>
              <es>¿Dónde están las tiendas libres de impuestos?</es>
            </translations>
          </line>
          <line by="B">
            <translations>
              <en>It is straight ahead</en>
              <ar>
                <x>إلى الأمام مباشرة</x>
                <r>ila alamam mubashara</r>
              </ar>
              <de>Einfach geradeaus</de>
              <pl>Tam, na wprost.</pl>
              <es>Es todo recto</es>
            </translations>
          </line>
          <line by="A">
            <translations>
              <en>Where is gate number seven?</en>
              <ar>
                <x>أين البوابة رقم سبعة؟</x>
                <r>ayna albawaba rakam saba'a?</r>
              </ar>
              <de>Wo ist der Flugsteig Nummer sieben?</de>
              <pl>Gdzie jest bramka numer siedem?</pl>
              <es>¿Dónde está la puerta de embarque número siete?</es>
            </translations>
          </line>
          <line by="B">
            <translations>
              <en>It is on your right.</en>
              <ar>
                <x>إنها على يمينك</x>
                <r>inaha a'ala yaminika</r>
              </ar>
              <de>Er ist auf der rechten Seite.</de>
              <pl>Po prawej stronie.</pl>
              <es>Está a su derecha.</es>
            </translations>
          </line>
        </lines>
      </dialog>
    </step>
  </steps>
</level>