<?xml version="1.0" encoding="utf-8"?>
<level xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../level.xsd">
  <levelInfo number="10">
    <name>
      <en>price</en>
      <ar>
        <x>السعر</x>
        <r>alsi'ar</r>
      </ar>
      <de>Preis</de>
      <pl>cena</pl>
      <es>Precio</es>
    </name>
  </levelInfo>
  <steps>
    <step>
      <word wid="1008" type="word">
        <translations>
          <en>Cheap</en>
          <ar>
            <x>رخيص</x>
            <r>rakhis</r>
          </ar>
          <de>Billig</de>
          <pl>Tani/tania</pl>
          <es>Barato</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1009" type="word">
        <translations>
          <en>Expensive</en>
          <ar>
            <x>غالي</x>
            <r>ghali</r>
          </ar>
          <de>Teuer</de>
          <pl>Drogi/droga</pl>
          <es>Caro</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1001" type="word">
        <translations>
          <en>This is an expensive watch!</en>
          <ar>
            <x>!إنها ساعة غالية</x>
            <r>inaha sa'a-ton ghaliya!</r>
          </ar>
          <de>Das ist eine teure Uhr!</de>
          <pl>To jest drogi zegarek!</pl>
          <es>¡Este reloj es caro!</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1002" type="word">
        <translations>
          <en>How much does this cost?</en>
          <ar>
            <x>كم سعره؟</x>
            <r>kam sia'roh?</r>
          </ar>
          <de>Wie viel kostet das?</de>
          <pl>Ile to kosztuje?</pl>
          <es>¿Cuánto cuesta esto?</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1003" type="word">
        <translations>
          <en>Will you give me discount?</en>
          <ar>
            <x>هل سوف تعطيني خصم؟</x>
            <r>hal saoufa tou'tini khasm?</r>
          </ar>
          <de>Geben Sie mir Rabatt?</de>
          <pl>Czy otrzymam rabat?</pl>
          <es>¿Me hará un descuento?</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1004" type="word">
        <translations>
          <en>Do you accept dollars?</en>
          <ar>
            <x>هل تقبل الدولار؟</x>
            <r>hal takbala al dolar?</r>
          </ar>
          <de>Akzeptieren Sie Dollar?</de>
          <pl>Czy przyjmujecie dolary?</pl>
          <es>¿Acepta dólares?</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1005" type="word">
        <translations>
          <en>I don't have enough money</en>
          <ar>
            <x>ليس لدي المال الكافي</x>
            <r>laysa ladaya almalu alkafi</r>
          </ar>
          <de>Ich habe nicht genug Geld</de>
          <pl>Nie mam tyle pieniędzy</pl>
          <es>No tengo suficiente dinero</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1006" type="word">
        <translations>
          <en>What other colors do you have?</en>
          <ar>
            <x>ماهي الألوان الأخرى التي لديك؟</x>
            <r>ma hiya alalwan alukhra alati ladayk?</r>
          </ar>
          <de>Welche anderen Farben haben Sie?</de>
          <pl>Jakie jeszcze kolory macie?</pl>
          <es>¿Qué otros colores tiene?</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1007" type="word">
        <translations>
          <en>Do you accept credit cards?</en>
          <ar>
            <x>هل تقبل البطاقة الإئتمانية؟</x>
            <r>hal taqbal albitaka-ta aliatimanya?</r>
          </ar>
          <de>Akzeptieren Sie Kreditkarten?</de>
          <pl>Czy akceptujecie karty kredytowe?</pl>
          <es>¿Acepta tarjeta de crédito?</es>
        </translations>
      </word>
    </step>
    <step>
      <multipleChoiceTest limitTestsBy="1">
        <choicesByRef>
          <choice ref="1008" />
          <choice ref="1009" />
        </choicesByRef>
      </multipleChoiceTest>
    </step>
    <step>
      <word wid="1010" type="word">
        <translations>
          <en>Watch</en>
          <ar>
            <x>ساعة</x>
            <r>sa'a</r>
          </ar>
          <de>Uhr</de>
          <pl>Zegarek</pl>
          <es>Reloj</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1011" type="word">
        <translations>
          <en>I am looking for a white suite.</en>
          <ar>
            <x>أبحث عن بدلة بيضاء</x>
            <r>abhathu a'an badla-ten bayda'</r>
          </ar>
          <de>Ich suche nach einer weißen Suite</de>
          <pl>Szukam białego garnituru.</pl>
          <es>Estoy buscando una suite blanca.</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1012" type="word">
        <translations>
          <en>Cash</en>
          <ar>
            <x>نقد</x>
            <r>naqd</r>
          </ar>
          <de>Bargeld</de>
          <pl>Gotówka</pl>
          <es>Dinero</es>
        </translations>
      </word>
    </step>
    <step>
      <word wid="1013" type="word">
        <translations>
          <en>Coat</en>
          <ar>
            <x>معطف</x>
            <r>mi'ataf</r>
          </ar>
          <de>Mantel</de>
          <pl>Płaszcz</pl>
          <es>Abrigo</es>
        </translations>
      </word>
    </step>
    <step>
      <multipleChoiceTest limitTestsBy="2">
        <choicesByRef>
          <choice ref="1013" />
          <choice ref="1012" />
          <choice ref="1010" />
        </choicesByRef>
      </multipleChoiceTest>
    </step>
    <step>
      <word wid="1014" type="word">
        <translations>
          <en>This car is cheap</en>
          <ar>
            <x>هذه السيارة رخيصة</x>
            <r>hathihi al sayara-to rakhisa</r>
          </ar>
          <de>Dieses Auto ist billig</de>
          <pl>Ten samochód jest tani</pl>
          <es>Este coche es barato</es>
        </translations>
      </word>
    </step>
    <step>
      <dialog did="1015">
        <among>
          <speaker who="A" gender="M"></speaker>
          <speaker who="B" gender="F"></speaker>
        </among>
        <lines>
          <line by="A">
            <translations>
              <en>How much is this coat?</en>
              <ar>
                <x>كم سعر هذا المعطف؟</x>
                <r>kam si'aru hatha al mi'ataf?</r>
              </ar>
              <de>Wie viel kostet dieser Mantel?</de>
              <pl>Ile kosztuje ten płaszcz?</pl>
              <es>¿Cuánto cuesta este abrigo?</es>
            </translations>
          </line>
          <line by="B">
            <translations>
              <en>It is $30.</en>
              <ar>
                <x>ثلاثون دولاراً</x>
                <r>thalathuna dolaran</r>
              </ar>
              <de>Er kostet 30 $.</de>
              <pl>30 USD.</pl>
              <es>Cuesta 30 $.</es>
            </translations>
          </line>
          <line by="A">
            <translations>
              <en>That is cheap, do you accept credit card?</en>
              <ar>
                <x>إنه رخيص، هل تقبلي بطاقة ائتمان؟</x>
                <r>inahu rakhis, hal taqbali bitaqata eiatiman?</r>
              </ar>
              <de>Das ist billig, akzeptieren Sie Kreditkarten?</de>
              <pl>Tani. Czy akceptujecie karty kredytowe?</pl>
              <es>Es barato, ¿acepta tarjeta de crédito?</es>
            </translations>
          </line>
          <line by="B">
            <translations>
              <en>Yes we do.</en>
              <ar>
                <x>نعم نقبلها</x>
                <r>na'am naqbaluha</r>
              </ar>
              <de>Ja, tun wir.</de>
              <pl>Tak.</pl>
              <es>Sí, la aceptamos.</es>
            </translations>
          </line>
        </lines>
      </dialog>
    </step>
  </steps>
</level>